# Global variables

## Formatting
# Italics markers, put these around text to italicize it
-i = {"*"}
# Bold markers, put these around text to bolden it
-b = {"**"}
# Underline markers, put these around text to underline it
-u = {"__"}
# Strikethrought markers, put these around text to strikethrought it
-s = {"~~"}
# Block Quote
-bq = {">"}
# Block Quote till end of message
-bqe = {">>>"}
# Spoiler Tags
-st = {"||"}
# Code line
-cl = {"`"}
# Code Block
-cb =  {"```"}
# Hashtag
-h = {"#"}